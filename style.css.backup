body {
    color:#f8f8f2;
    background:#272822;
    margin: auto;
    font-family:Hack;
}

.pbetter {
    font-size:14px;
}
.h2better {
    color:#a6e22e;
}

body {
    max-width: 860px;
    overflow-x:hidden;
}

.img-with-text {
    float: right;
    text-align: center;
    width: 256px;
    font-size:10px;
}

.img-with-text img {
    height: 256px;
    width: 256px;
    float: right;
    max-width: 300px;
    box-shadow:0px 0px 7px #000000;
}

.image {
    border: 1px solid #f8f8f2;
}

.img-with-text p {
    margin-top: 262px
}

.lolprev {
    background:#1c1d1a;
	padding: 10px;
    width: 170px;
    border-radius: 7px;
    display: inline-block;
}

.lolprev ul {
	margin: 0;
	list-style: none;
	padding: 0;
}

.flex-container {
    display: flex;
    gap: 0px 10px;
}

.flex-container .flex-item {
    background:#1c1d1a;
	padding: 10px;
    width: 170px;
    border-radius: 7px;
    display: inline-block;
}

.flex-container .flex-item ul {
    margin: 0;
	list-style: none;
	padding: 0;
}

li {
    font-size:12px;
    margin: 8px 0;
}

.homepagelist {
    margin: 0px 0;
}

.invert{
    -webkit-filter: invert(100%); /* safari 6.0 - 9.0 */
    filter: invert(100%);
}

.warning {
    color: #f92672;
    font-size:12px;
}

.ytchannels {
    font-size:8px;
}

a {
    color: #66d9ef;
}

.yt {
    width: 535px;
}